class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:

        left = right = 0
        maxLen = 0
        n = len(s)

        seen = set()

        while right < n:
            if s[right] not in seen:
                seen.add(s[right])
                maxLen = max(maxLen, right-left+1)
                right+=1
            else:
                seen.remove(s[left])
                left+=1
        return maxLen

        
