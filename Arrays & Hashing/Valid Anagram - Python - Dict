class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        
        if len(s) != len(t):
            return False
        
        unique = {}

        for i in range(len(s)):
            unique[s[i]] = unique.get(s[i], 0) + 1

        for i in range(len(t)):
            if not t[i] in unique:
                return False
            unique[t[i]] -= 1

        for count in unique.values():
            if count!=0:
                return False
        return True
